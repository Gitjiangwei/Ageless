<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ageless.mapper.CategoryTwoMapper">
    <select id="Twolist" resultType="CategoryTwo">
        select id,categoryName,parentId from categorytwo where parentId=#{id}
    </select>
    <insert id="insertTwo" parameterType="CategoryTwo">
        insert into categorytwo(categoryName,parentId) value(#{categoryName},#{parentId})
    </insert>
    <update id="updateTwo" parameterType="CategoryTwo">
        update categorytwo set categoryName=#{categoryName},parentId=#{parentId} where id=#{id}
    </update>
</mapper>
